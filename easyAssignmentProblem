std::vector<int> easyAssignmentProblem(std::vector<std::vector<int>> a) {
vector<int>v(2,1);
int t1,t2;
if(a[0][0]+a[1][1]>=a[1][0]+a[0][1])
    v[1]=2;
else v[0]=2;
//cout<<a[0][0]<<' '<<a[0][1]<<'\n';
//cout<<a[1][0]<<' '<<a[1][1]<<'\n';
return v;
}

